#Guess the number
def guess_number (target):
    number = -1
    while number != target:
        number = int(input("Guess a number between 1-100: "))
        if number < target:
            print("Thats too low!")
            
        if number == target:
            print("Correct")
            
        if number > target:
            print ("Thats too high!")
