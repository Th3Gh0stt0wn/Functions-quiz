def ExerciseCheck():
    exercise = int(input("How many minutes of exercise do you get every week?: "))
    if  exercise < 20:
        print("That's not enough exercise!")
    elif exercize < 100:
        print("Get a bit more exercise!")
    elif exercize < 500:
        print("That's the perfect amount of exercise!")
    elif exercize > 500:
        print("Thats alot! Drinks Lots of water and protein!")

ExerciseCheck()

print()

