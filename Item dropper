import random
#function deff
def Itemdropper():
    num = random.randrange(0,100)
    if num < 20:
        print("You find a potion")
    elif num < 50:
        print("You got a cupcake!")
    elif num > 75:
       print("You find a dirty sock")
    else:
        print("Theres nothing there")
#End of function
        
while True:
    Itemdropper()
    choice = input("Enter any key to continue..")

